<div class="container mt-5 ">
    <h1>Equipos de La Liga</h1>
    <div class="row mt-5">
        {{#each team}}
        <div class="col-md-3">
            <article class="teams-card mb-5">
                <h4 class="text-center">{{fullName}}</h4>
                <hr>
                <img src="{{shield_png}}" alt="logo">
            </article>
            <a href="/plantilla/{{id_comp}}" class="btn btn-dark">plantilla</a>
        </div>
        {{/each}}
    </div>
</div>